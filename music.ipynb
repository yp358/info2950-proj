    "spotify['artists'] = spotify['artists'].str.replace('$', 's')                                        \n",
    "\n",
    "\n",
    "billboard['Artist'] = billboard['Artist'].str.replace('Ø', 'o')\n",
    "spotify['artists'] = spotify['artists'].str.replace('Ø', 'o')\n",
    "\n",
    "\n",
    "billboard['Artist'] = billboard['Artist'].str.replace('&', ', ')\n",
    "spotify['artists'] = spotify['artists'].str.replace('&', ', ') \n",
    "\n",
    "billboard['Artist'] = billboard['Artist'].str.replace(' and ', ', ')\n",
