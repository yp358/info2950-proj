    "\n",
    "\n",
    "billboard['Song'] = billboard['Song'].str.replace('&', ',')\n",
    "spotify['name'] = spotify['name'].str.replace('&', ',') \n",
    "\n",
    "\n",
    "# Drop the temporary FeatureArtist column\n",
    "billboard.drop(columns=['FeatureArtist'], inplace=True)\n",
